[alias]
    st = status -s
    cl = clone
    ci = commit
    co = checkout
    br = branch
    r = reset
    cp = cherry-pick
    gr = grep -Ii
    sl = stash list
    sa = stash apply
    ss = stash save
    unstage = reset HEAD --
    last = log -1 HEAD
    ls = log --pretty=format:\"%C(yellow)%h%Cred%d\\\\ %Creset%s%Cblue\\\\ [%cn]\" --decorate
    ll = log --pretty=format:\"%C(yellow)%h%Cred%d\\\\ %Creset%s%Cblue\\\\ [%cn]\" --decorate --numstat
    lsd = log --pretty=format:\"%C(yellow)%h\\\\ %ad%Cred%d\\\\ %Creset%s%Cblue\\\\ [%cn]\" --decorate --date=short
    lsdr = log --pretty=format:\"%C(yellow)%h\\\\ %ad%Cred%d\\\\ %Creset%s%Cblue\\\\ [%cn]\" --decorate --date=relative
    sl = log --oneline --decorate
    filelog = log -u
    la = config --get-regexp ^alias\\.
    llg = log --oneline --decorate --graph
    bs = for-each-ref --format=\"%(refname:short) %(push:track)\" refs/heads